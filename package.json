{
  "name": "vue2-vite2",
  "version": "0.0.0",
  "module": "./dist/vite-vue2-starter2.es.js",
  "main": "./dist/vite-vue2-starter2.umd.js",
  "exports": {
    ".": {
      "import": "./dist/vite-vue2-starter2.es.js",
      "require": "./dist/vite-vue2-starter2.umd.js"
    },
    "./*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && sass src/assets/main.scss dist/index.css",
    "test": "jest tests/unit",
    "prepare": "husky install",
    "lint": "eslint --ext .ts,vue --fix --ignore-path .gitignore .",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "dependencies": {
    "vite-plugin-vue2": "^1.4.0",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "husky": "^5.1.3",
    "@vitejs/plugin-vue": "^2.0.1",
    "rollup-plugin-typescript2": "^0.31.1",
    "vite": "^2.0.5",
    "@babel/core": "^7.10.3",
    "@vue/babel-preset-app": "^4.4.6",
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "moment": "^2.27.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2"
  }
}
